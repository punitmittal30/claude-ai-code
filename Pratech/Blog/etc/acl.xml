<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::content">
                    <resource id="Magento_Backend::content_elements">
						<resource id="Pratech_Blog::Category" title="Category" sortOrder="10">
							<resource id="Pratech_Blog::Category_save" title="Save Category" sortOrder="10"/>
							<resource id="Pratech_Blog::Category_delete" title="Delete Category" sortOrder="20"/>
							<resource id="Pratech_Blog::Category_update" title="Update Category" sortOrder="30"/>
							<resource id="Pratech_Blog::Category_view" title="View Category" sortOrder="40"/>
						</resource>
						<resource id="Pratech_Blog::Tag" title="Tags" sortOrder="10">
							<resource id="Pratech_Blog::Tag_save" title="Save Tag" sortOrder="10"/>
							<resource id="Pratech_Blog::Tag_delete" title="Delete Tag" sortOrder="20"/>
							<resource id="Pratech_Blog::Tag_update" title="Update Tag" sortOrder="30"/>
							<resource id="Pratech_Blog::Tag_view" title="View Tag" sortOrder="40"/>
						</resource>
						<resource id="Pratech_Blog::Comment" title="Comments" sortOrder="10">
							<resource id="Pratech_Blog::Comment_save" title="Save Comment" sortOrder="10"/>
							<resource id="Pratech_Blog::Comment_delete" title="Delete Comment" sortOrder="20"/>
							<resource id="Pratech_Blog::Comment_update" title="Update Comment" sortOrder="30"/>
							<resource id="Pratech_Blog::Comment_view" title="View Comment" sortOrder="40"/>
						</resource>
					</resource>
        		</resource>
			</resource>
		</resources>
	</acl>
</config>
