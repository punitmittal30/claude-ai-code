<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Pratech_Banners::pratech" title="Pratech"
                          translate="true" sortOrder="200">
                    <resource id="Pratech_Banners::banner_slider" title="Banner Management"
                              translate="true" sortOrder="100">
                        <resource id="Pratech_Banners::slider" title="Sliders"
                                  translate="true" sortOrder="10"/>
                        <resource id="Pratech_Banners::banner" title="Banners"
                                  translate="true" sortOrder="20"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
