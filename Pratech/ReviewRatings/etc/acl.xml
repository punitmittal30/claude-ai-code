<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Pratech_Banners::pratech" title="Pratech" sortOrder="150">
                    <resource id="Pratech_ReviewRatings::reviews" title="Import Review" translate="title"
                              sortOrder="10">
                        <resource id="Pratech_ReviewRatings::reviews_import" title="Import Reviews" translate="title"
                                  sortOrder="10"/>
                        <resource id="Pratech_ReviewRatings::keywords" title="Keywords" translate="title"
                                  sortOrder="50"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::marketing">
                    <resource id="Magento_Backend::marketing_user_content">
                        <resource id="Pratech_ReviewRatings::review_media" title="Review Media" translate="title" sortOrder="50"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>